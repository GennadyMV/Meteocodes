<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Codes.Models" assembly="Codes">
  <class name="Measurement" table="Measurment">
    <id name="ID" unsaved-value="0">
      <column name="ID" not-null="true" />
      <generator class="identity"/>
    </id>
    <property name="created_at" />
    <property name="updated_at" />
    <property name="DD" />
    <property name="GG" />
    <many-to-one name="Station" column="Station_ID" />
    <property name="surface_pressure" />
    <property name="surface_temperature" />
    <property name="surface_dewpoint" />
    <property name="surface_wind" />
    <property name="surface_windspeed" />
    <property name="surface_windunit" />
  </class>
</hibernate-mapping>